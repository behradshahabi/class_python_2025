nomarat = [20, 14, 13, 15, 20, 4, 6, 9, 20, 11, 16, 7.5]
summation = 0
ghaboolshode = []
mardodi = []

for n in nomarat:
    summation += n
miangin = summation / len(nomarat)
for n in nomarat:
    if n < 12 :
     mardodi.append(n)
    else:
        ghaboolshode.append(n)
print (f"miangin : {miangin}")
print (f"mardoodi ha : {mardodi}")
print (f"ghabool shode ha : {ghaboolshode}")